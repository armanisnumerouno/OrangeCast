<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/library"
    app:startDestination="@id/libraryScreen">

    <fragment
        android:id="@+id/libraryScreen"
        android:name="com.example.orangecast.ui.library.LibraryFragment">

        <action
            android:id="@+id/artistScreen"
            app:destination="@id/artistScreen" >

            <argument
                android:name="artistFeedUrl"
                app:argType="string" />
        </action>

        <action
            android:id="@+id/discoverScreen"
            app:destination="@id/discoverScreen"/>
    </fragment>
</navigation>